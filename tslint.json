{
  "rules": {
    "ordered-imports": [false],
    "semicolon": [true, "never"],
    "member-access": [false],
    "member-ordering": [false],
    "trailing-comma": [true, { "singleline": "never", "multiline": "always" }],
    "no-empty": [false],
    "max-classes-per-file": [false],
    "no-empty-interface": [false]
  }
}
